   "source": [
    "import re\n",
    "def chk_palindrome(text):\n",
    "    forwards= ''.join(re.findall(r'[a-z]+',text.lower()))\n",
    "    backwards= forwards[::-1]\n",
    "    return forwards== backwards\n",
    "     \n",
    "\n",
    "\n",
    "    "
   ]
